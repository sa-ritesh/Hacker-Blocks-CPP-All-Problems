#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int space=1;
    int a=-1,b=1;
    int sum=a+b;
    for(int i=1; i<=n; i++)
    {

        for(int j=1; j<=i; j++)
        {
            cout<<sum<<" ";
            a=b;
            b=sum;
            sum=a+b;
        }
        cout<<endl;
    }

}
