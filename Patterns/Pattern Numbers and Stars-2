#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int space=1;
    for(int i=1; i<=n; i++)
    {
        for(int j=1; j<=i; j++)
        {
            cout<<j;
        }
        for(int k=i; k<n; k++)
        {
            cout<<"*";
        }
        cout<<endl;
    }
}
